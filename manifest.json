{
    "manifest_version": 3,
    "name": "Job Application Tracker",
    "version": "1.0",
    "description": "Reads and prints the content of the current webpage.",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_icon": {
        "16": "pineapple.png",
        "48": "pineapple.png",
        "128": "pineapple.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
}